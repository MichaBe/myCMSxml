	<headline>myCMSxml</headline>
	<description>Die Anmeldeseite für myCMSxml</description>
	<text>
		<![CDATA[
			<p align="center">
				<form action="cms/management/login.php" method="post">
				<table>
					<tr>
						<td>Benutzername:</td>
						<td><input type="text" name="username"/></td>
					</tr>
					<tr>
						<td>Passwort:</td>
						<td><input type="password" name="passwort"/></td>
					</tr>
				</table>
				<input type="submit" value="Anmelden"/>
				</form>
			</p>
		]]>
	</text>
